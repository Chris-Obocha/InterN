{
  "expo": {
    "name": "InterN",
    "slug": "InterN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-letter-blue.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-letter-blue.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.christopherobocha.intern2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "hpft.hweics.intern"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
